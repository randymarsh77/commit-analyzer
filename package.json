  },
  "repository": {
    "type": "git",
    "url": "https://github.com/randymarsh/commit-analyzer.git"
  },
  "scripts": {
    "codecov": "codecov -f coverage/coverage-final.json",
    "lint": "xo",
    "pretest": "npm run lint",
    "semantic-release": "semantic-release",
    "test": "nyc ava -v"
  },
  "xo": {
    "prettier": true,
    "space": true
